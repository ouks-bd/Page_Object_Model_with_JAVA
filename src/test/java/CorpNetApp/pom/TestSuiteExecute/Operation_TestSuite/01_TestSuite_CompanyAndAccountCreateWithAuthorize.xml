<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <test verbose="2" preserve-order="true" name="Create new Company And Approve">
        <classes>
            <class name="CorpNetApp.pom.test.testOperation.TestCases_CompanyManagement.TestCases_CreateCompany.TC_1_CreateCompanyWithAllValidData">
                <methods>
                    <include name="addNewCompanyShouldSuccess"/>
                </methods>
            </class>
            <class name="CorpNetApp.pom.test.testOperation.TestCases_CompanyManagement.TestCases_CreateCompany.TC_2_ApproveCompanyWithValidData">
                <methods>
                    <include name="unauthorizedCompanyAuthorizeSuccess"/>
                </methods>
            </class>
            <class name="CorpNetApp.pom.test.testOperation.TestCases_CompanyManagement.TestCases_CreateAccount.TC_CreateCompanyAccountWithValidData">
                <methods>
                    <include name="showCompanyAccountDetailsAndCreateSuccess"/>
                </methods>
            </class>
            <class name="CorpNetApp.pom.test.testOperation.TestCases_CompanyManagement.TestCases_CreateAccount.TC_ApproveCompanyAccount">
                <methods>
                    <include name="approveCompanyAccountSuccess"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>


